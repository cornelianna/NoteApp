    <PackageReference Include="coverlet.collector" Version="6.0.0" />
  </ItemGroup>


  <ItemGroup>
    <Using Include="Xunit" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\NoteApp.csproj" />
  </ItemGroup>

</Project>